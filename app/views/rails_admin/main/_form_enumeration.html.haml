= form.select field.method_name, field.enum, {:include_blank => true}, field.html_attributes.reverse_merge({ :placeholder => t('admin.index.search') })
= javascript_tag do
  :plain
    jQuery(function($) {
      if(!$("##{form.dom_id(field)}").parents(".fields_blueprint").length) {
        $("##{"#{form.dom_id(field)}"}").filteringSelect({});
      }
    })
